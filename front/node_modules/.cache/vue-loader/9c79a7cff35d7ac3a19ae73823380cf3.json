ภาพหรือเพิ่มบัญชี</h4>\r\n            <button @click=\"Register\">\r\n              <span>\r\n                <v-card width=\"256\" class=\"mx-auto\" style=\"position: relative; top: 20px;\">\r\n                  <v-img class=\"white--text align-end d-flex align-center\" height=\"200px\">\r\n                    <v-icon style=\"font-size: 50px\">mdi-plus-circle</v-icon>\r\n                  </v-img>\r\n\r\n                  <v-card-text class=\"text--primary\">\r\n                    <div>\r\n                      <h2>เพิ่มบัญชี</h2>\r\n                    </div>\r\n                  </v-card-text>\r\n                </v-card>\r\n              </span>\r\n            </button>\r\n          </v-col>\r\n        </v-row>\r\n      </v-col>\r\n      <!-- LOGIN FROM -->\r\n      <v-col cols=\"12\" sm=\"5\">\r\n        <v-row no-gutters>\r\n          <v-card width=\"450\" class=\"mx-auto\" style=\"position: relative; top: 20px;\">\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"Email*\" required v-model=\"emailuser\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field label=\"Password*\" type=\"password\" required v-model=\"password\"></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n            <v-card-actions>\r\n              <v-btn block color=\"primary\" dark @click=\"Login\">\r\n                <h2>เข้าสู่ระบบ</h2>\r\n              </v-btn>\r\n            </v-card-actions>\r\n            <br />\r\n            <hr />\r\n            <br />\r\n            <v-card-actions>\r\n              <v-col cols=\"12\" class=\"text-center\">\r\n                <v-btn large color=\"success\" dark @click=\"Register\">\r\n                  <h3>สมัครบัญชีใหม่</h3>\r\n                </v-btn>\r\n              </v-col>\r\n            </v-card-actions>\r\n            <br />\r\n          </v-card>\r\n        </v-row>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport AuthService from \"@/services/AuthService.js\";\r\nimport Vue from \"vue\";\r\n\r\nexport default Vue.extend({\r\n  data: () => ({\r\n    msg:\"\",\r\n    emailuser: \"\",\r\n    password: \"\",\r\n  }),\r\n  methods: {\r\n    async Register() {\r\n      if (this.$refs.form.validate()) {\r\n        try {\r\n          const credentials = {\r\n            emailuser: this.emailuser,\r\n            password: this.password\r\n          };\r\n          const response = await AuthService.signUp(credentials);\r\n          this.msg = response.msg;\r\n          this.$refs.form.reset();\r\n        } catch (error) {\r\n          alert((this.msg = error.response.data.msg));\r\n        }\r\n      }\r\n    },\r\n    async Login() {\r\n      if (this.$refs.form.validate()) {\r\n        try {\r\n          const credentials = {\r\n            emailuser: this.emailuser,\r\n            password: this.password\r\n          };\r\n          const response = await AuthService.login(credentials);\r\n          this.msg = response.msg;\r\n          const token = response.token;\r\n          const user = response.user;\r\n          this.$store.dispatch(\"login\", { token, user });\r\n        } catch (error) {\r\n          alert((this.msg = error.response.data.msg));\r\n          //this.snackbar = true\r\n          //this.msg = error.response.data.msg;\r\n        }\r\n      }\r\n    }\r\n  }\r\n});\r\n</script>\r\n"]}]}